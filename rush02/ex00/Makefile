NAME = "rush-02"

SRC = *.c

all : $(NAME)

$(NAME):
	gcc -o $(NAME) $(SRC)

clean:
	/bin/rm -f *.o
fclean: clean
	/bin/rm -f $(NAME)

re : clean all
